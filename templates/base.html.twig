<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body></body>
</html>
<html id="theme" data-theme="dark">

	<main class="container">
		<nav>
			<ul>
				<li>
					<i class="fas fa-head-side-virus"></i> <strong>Maid assist</strong>
				</li>
			</ul>
			<ul>
				<li><a href="{{ path('app_home') }}">home</a></li>
				<li><a href="{{ path('app_intervention_index') }}">My intervention</a></li>
				<li><a href="{{ path('app_customer_index') }}">My customers</a></li>
				<li><a href="#">My finances</a></li>
				<li><a href="{{ path('admin') }}"><i class="fas fa-user"></i></a></li>
			</ul>
		</nav>
		{% block body %}{% endblock %}
	</main>
</html></body></html>
